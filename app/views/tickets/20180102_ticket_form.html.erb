<%= form_for [@customer, @store, @ticket] do |f| %>
<%= render 'layouts/errMsg', obj: @ticket %>
  <table class="table table-bordered ">
    <tbody>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :title, class:"control-label" %></td>
          <td><%= f.text_field :title, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

<!-- コメント開始 -->
<% if false %>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :status, class:"control-label" %></td>
          <td><%= f.text_field :status, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :sender, class:"control-label" %></td>
          <td><%= f.text_field :sender, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :received_user_id, class:"control-label" %></td>
          <td><%= f.text_field :received_user_id, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :assign_user_id, class:"control-label" %></td>
          <td><%= f.text_field :assign_user_id, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :receive_date, class:"control-label" %></td>
          <td><%= f.text_field :receive_date, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :receive_time, class:"control-label" %></td>
          <td><%= f.text_field :receive_time, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :close_date, class:"control-label" %></td>
          <td><%= f.text_field :close_date, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :close_time, class:"control-label" %></td>
          <td><%= f.text_field :close_time, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :esc_date, class:"control-label" %></td>
          <td><%= f.text_field :esc_date, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :esc_time, class:"control-label" %></td>
          <td><%= f.text_field :esc_time, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :limit_date, class:"control-label" %></td>
          <td><%= f.text_field :limit_date, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :content, class:"control-label" %></td>
          <td><%= f.text_field :content, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :detail, class:"control-label" %></td>
          <td><%= f.text_field :detail, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

      <div class="form-group">
        <tr>
          <td class="active text-right"><%= f.label :result, class:"control-label" %></td>
          <td><%= f.text_field :result, autofocus: true, class:"form-control" %></td>
        </tr>
      </div>

<% end %>
<!-- コメント終了 -->

    </tbody>
  </table>

  <div class="actions col-md-offset-9 col-md-3">
    <%= f.submit class:"btn btn-lg btn-primary btn-block" %>
  </div>

<% end %>
